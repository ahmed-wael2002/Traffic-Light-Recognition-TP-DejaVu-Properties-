pred isStateEqual(recognizedState, actualState) = recognizedState = actualState

prop StateRecognitionCheck :
    forall recState. forall actState .
        checkStateRecognition(recState, actState) -> isStateEqual(recState, actState)

prop AccuracyLimitCheck :
    forall accuracy. forall limit .
        checkAccuracyLimit(accuracy, limit) -> accuracy <= limit

